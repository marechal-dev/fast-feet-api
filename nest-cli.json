{
	"$schema": "https://json.schemastore.org/nest-cli",
	"collection": "@nestjs/schematics",
	"sourceRoot": "src",
	"entryFile": "Infra/main",
	"compilerOptions": {
		"assets": [
			{
				"include": "Infra/I18n/**/*",
				"watchAssets": true
			}
		],
		"builder": "swc",
		"typeCheck": true,
		"deleteOutDir": true
	}
}