	"scripts": {
		"check": "biome check --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"build": "nest build",
		"start": "nest start",
		"start:docs": "npx @compodoc/compodoc -p tsconfig.json -s",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:ui": "vitest --ui",
		"test:cov": "vitest run --coverage",
		"test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
		"test:e2e": "vitest run --config ./vitest.config.e2e.ts",
		"test:e2e:watch": "vitest --config ./vitest.config.e2e.ts",
		"test:e2e:cov": "vitest run --config ./vitest.config.e2e.ts --coverage",
		"test:e2e:debug": "vitest --config ./vitest.config.e2e.ts --inspect-brk --inspect --logHeapUsage --threads=false",
		"prepare": "husky"
	},
	"dependencies": {
		"@nestjs/common": "^10.0.0",
